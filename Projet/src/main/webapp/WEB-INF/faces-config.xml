<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<navigation-rule>
		<display-name>transferForm.xhtml</display-name>
		<from-view-id>/transferForm.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>#{transferBean.performTransfer}</from-outcome>
			<to-view-id>/showTransferResult.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>welcome.xhtml</display-name>
		<from-view-id>/welcome.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>lookArtist</from-outcome>
			<to-view-id>/artists.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
